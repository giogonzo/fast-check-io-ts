  "author": "Giovanni Gonzaga <giovanni@buildo.io>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "fast-check": "^1.16.0",
    "fp-ts": "^2.0.3",
    "io-ts": "^2.0.1",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "fast-check": "^1.16.0",
    "io-ts": "^2.0.0",
    "fp-ts": "^2.0.0"
  },
  "jest": {
    "preset": "ts-jest"
  }
